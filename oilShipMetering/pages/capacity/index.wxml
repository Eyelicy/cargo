<!--pages/capacity/index.wxml-->
<form bindsubmit="formSubmit">
  <view class='content'>
    <view class='title'>
      <text>修正后空距 <text>{{correntkong}}</text></text>
    </view>
    <view class='rightColumn1'>
      <view class='column'>
        <text>空高1</text>
        <input focus="{{value1}}" name="ullage1" type="digit" placeholder='请输入空高1' placeholder-class="inputPl" value="{{list.ullage1}}"></input>
      </view>
      <view class='column'>
        <text>容量1</text>
        <input focus="{{value2}}" name="capacity1" type="digit" placeholder='请输入容量1' placeholder-class="inputPl" value="{{list.capacity1}}"></input>
      </view>
      <view class='column'>
        <text>空高2</text>
        <input focus="{{value3}}" name="ullage2" type="digit" placeholder='请输入空高2' placeholder-class="inputPl" value="{{list.ullage2}}"></input>
      </view>
      <view class='column'>
        <text>容量2</text>
        <input focus="{{value4}}" name="capacity2" type="digit" placeholder='请输入容量2' placeholder-class="inputPl" value="{{list.capacity2}}"></input>
      </view>
    </view>
  </view>
  <button class='sub' form-type="submit">提交</button>
</form>